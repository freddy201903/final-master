<body background="~/project_final/images/bg-02.jpg" >
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
                <form method="post" class="login100-form validate-form">
                    <span class="login100-form-title p-b-49">
                        <a href="./Index"><img src="~/project_final/images/logoRed_sm.png"></a>
                    </span>

                    <div class="wrap-input100 validate-input m-b-23" data-validate="帳號必須填寫！">
                        <span class="label-input100">帳號</span>
                        <input class="input100" type="text" name="Account" placeholder="請輸入帳號">
                        <span class="focus-input100" data-symbol="&#xf206;"></span>
                    </div>

                    <div class="wrap-input100 validate-input" data-validate="密碼必須填寫！">
                        <span class="label-input100">密碼</span>
                        <input class="input100" type="password" name="Password" placeholder="請輸入密碼">
                        <span class="focus-input100" data-symbol="&#xf190;"></span>
                    </div>
                    
                    <div class="text-right p-t-8 p-b-31">

                        <!-- <a href="#">
                            Forgot password?
                        </a> -->
                        
                    </div>

                    <div class="container-login100-form-btn" style="padding-bottom: 2em">
                        <div class="wrap-login100-form-btn">
                            <div class="login100-form-bgbtn"></div>
                            <input type="submit" value="登    入" class="login100-form-btn">
                            @ViewBag.Message
                        </div>
                    </div>

                    <div class="container-login200-form-btn">
                        <div class="wrap-login200-form-btn">
                            <div class="login200-form-bgbtn"></div>
                            <button type="button" class="login200-form-btn" data-toggle="modal" data-target="#signUp">
                                註    冊
                            </button>
                        </div>
                    </div>


                    <!-- 排版用 -->
                    <div class="txt1 text-center p-t-54 p-b-20">
                        <!-- <span>
                            Or Sign Up Using
                        </span> -->
                    </div>

                    <div class="flex-c-m">
                        <a href="#" class="login100-social-item bg1">
                            <i class="fa fa-facebook"></i>
                        </a>

                        <a href="#" class="login100-social-item bg3">
                            <i class="fa fa-google"></i>
                        </a>
                    </div>

                    <div class="flex-col-c p-t-20">
                        <span class="txt1">
                            <a href="./index" class="txt2">回到首頁</a>
                        </span>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <div id="dropDownSelect1"></div>

